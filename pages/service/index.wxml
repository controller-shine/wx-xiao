<view class="container">
	<view class="bg" style='width:100%;min-height:100%;background-image:url("http://49.234.18.144/jx/public/Uploads/2020-03-06/1583514373123456.png");background-size:100% 100%;'>
		<view class="page-body" style="margin-left:5%; width:90%;">
			<swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" circular="{{circular}}" vertical="{{vertical}}" interval="{{interval}}" duration="{{duration}}" previous-margin="{{previousMargin}}px" next-margin="{{nextMargin}}px">
				<block wx:for="{{swiperList}}" wx:key="id">
					<swiper-item>
						<view class="swiper-item {{item}}">
							<image mode="widthFix" src="http://49.234.18.144/jx/public{{item.img}}"></image>
						</view>
					</swiper-item>
				</block>
			</swiper>
		</view>
	</view>
	<view>
		<view class="leixin">
			<van-row class="leixin_top">
				<van-col offset="1" span="1">
					<image style="margin-left:20%;margin-top: 40rpx;height: 16px;width: 3px;}" src="../../img/index/图层 6.png"></image>
				</van-col>
				<van-col span="12">
					<view style="margin-top:30rpx">选择服务类型</view>
				</van-col>
			</van-row>
			<van-row gutter="20" class="leixin_an">
				<van-radio-group value="{{ radio }}" bind:change="onChange">
					<view class="leixin_anniu">
						<van-col span="7">
							<van-radio name="1" label-disabled>维修</van-radio>
						</van-col>
						<van-col span="7">
							<van-radio name="2" label-disabled>安装</van-radio>
						</van-col>
						<van-col span="7">
							<van-radio name="3" label-disabled>保养</van-radio>
						</van-col>
					</view>
				</van-radio-group>
			</van-row>
		</view>
		<view class="xinxi">
			<van-row class="xinxi_top">
				<van-col offset="1" span="1">
					<image style="margin-left:20%;margin-top: 40rpx;height: 16px;width: 3px;}" src="../../img/index/图层 6.png"></image>
				</van-col>
				<van-col span="12">
					<view style="margin-top:30rpx">详细信息</view>
				</van-col>
			</van-row>
			<view class="biaodan">
				<van-row>
					<van-col span="8">
						<view style="margin-left:20rpx">联系人电话</view>
					</van-col>
					<van-col span="14">
						<view class="weui-cells weui-cells_after-title">
							<view class="weui-cell weui-cell_input">
								<input class="weui-input" maxlength="10"/>
							</view>
						</view>
					</van-col>
				</van-row>
			</view>
			<view class="biaodan">
				<van-row>
					<van-col span="8">
						<view style="margin-left:20rpx">联系人姓名</view>
					</van-col>
					<van-col span="14">
						<view class="weui-cells weui-cells_after-title">
							<view class="weui-cell weui-cell_input">
								<input class="weui-input" maxlength="10"/>
							</view>
						</view>
					</van-col>
				</van-row>
			</view>
			<view class="biaodan">
				<van-row>
					<van-col span="8">
						<view style="margin-left:20rpx">服务地址</view>
					</van-col>
					<van-col span="14">
						<view class="weui-cells weui-cells_after-title">
							<view class="weui-cell weui-cell_input">
								<input class="weui-input" maxlength="10"/>
								<image style="width:30rpx;height:24rpx;" src="../../img/service/定位.png"></image>
							</view>
						</view>
					</van-col>
				</van-row>
			</view>
			<view class="biaodan">
				<van-row>
					<van-col span="8">
						<view style="margin-left:20rpx">产品类型</view>
					</van-col>
					<van-col span="14">
						<view class="weui-cells weui-cells_after-title">
							<view class="weui-cell weui-cell_input">
								<input class="weui-input" maxlength="10"/>
							</view>
						</view>
					</van-col>
				</van-row>
			</view>
			<view class="biaodan">
				<van-row>
					<van-col span="8">
						<view style="margin-left:20rpx">品牌及型号</view>
					</van-col>
					<van-col span="14">
						<view class="weui-cells weui-cells_after-title">
							<view class="weui-cell weui-cell_input">
								<input class="weui-input" maxlength="10"/>
							</view>
						</view>
					</van-col>
				</van-row>
			</view>
			<view class="biaodan">
				<van-row>
					<van-col span="8">
						<view style="margin-left:20rpx">购机日期</view>
					</van-col>
					<van-col span="14">
						<view class="weui-cells weui-cells_after-title">
							<view class="weui-cell weui-cell_input">
								<input class="weui-input" maxlength="10"/>
								<image style="width:36rpx;height:24rpx;" src="../../img/service/时间 (1).png"></image>
							</view>
						</view>
					</van-col>
				</van-row>
			</view>
			<view class="biaodan">
				<van-row>
					<van-col span="8">
						<view style="margin-left:20rpx">预约时间</view>
					</van-col>
					<van-col span="14">
						<view class="weui-cells weui-cells_after-title">
							<view class="weui-cell weui-cell_input">
								<input class="weui-input" maxlength="10"/>
								<image style="width:40rpx;height:30rpx;" src="../../img/service/时间.png"></image>
							</view>
						</view>
					</van-col>
				</van-row>
			</view>
			<view>
				<van-row class="tijiao">
					<van-col offset="5" span="14"><image src="../../img/service/组 2.png"></image></van-col>
				</van-row>
			</view>
		</view>
	</view>
</view>